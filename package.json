{
  "name": "ts-sql-codegen",
  "version": "3.3.0",
  "description": "Generates ts-sql-query table mappings from tbls schema output",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "pnpm run build:ts && pnpm run copy:templates && pnpm run build:docs",
    "build:ts": "tsc",
    "build:docs": "typedoc --readme none src/index.ts",
    "copy:templates": "recursive-copy -w src dist -f \"*.hbs\"",
    "clean": "rimraf dist test/generated",
    "test": "mocha -r ts-node/register -r mocha-snap \"./test/**/*.ts\""
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "sql",
    "code-generator",
    "ts-sql-query",
    "query-builder"
  ],
  "author": {
    "name": "Lorefnon",
    "email": "lorefnon@tutanota.com",
    "url": "https://lorefnon.me"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.182",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.41",
    "@types/pg": "^8.6.5",
    "@types/rimraf": "^3.0.2",
    "better-sqlite3": "^7.5.3",
    "mocha": "^10.0.0",
    "mocha-snap": "^4.3.0",
    "pg": "^8.7.3",
    "recursive-copy-cli": "^1.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "ts-sql-query": "^1.28.0",
    "typedoc": "^0.22.17",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "ts-sql-query": "^1.28.0"
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "hbs-dedent-helper": "^0.3.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "ts-pattern": "^4.0.3",
    "zod": "^3.17.3"
  }
}
